using System;
using System.Collections;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace SimonRadford.Site.Models
{
    public class Manafacturer
    {
        public virtual int ManafacturerId { get; set; }
        public virtual string Name { get; set; }
